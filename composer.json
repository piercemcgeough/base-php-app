{
    "name": "piercemcgeough/base-app",
    "version": "1.1.0",
    "description": "Base PHP app ready to rock with PHPUnit and PSR-4 autoloading.",
    "type": "project",
    "require": {
        "php": "8.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "10.4.*",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.38"
    },
    "autoload": {
        "psr-4": {
            "PierceMcGeough\\BaseApp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PierceMcGeough\\BaseApp\\Tests\\Unit\\": "tests/Unit"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/piercemcgeough/base-app.git"
    },
    "keywords": ["php", "base-app", "skeleton", "PSR-4", "PHPUnit"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pierce McGeough",
            "email": "pmcgeough@hotmail.co.uk"
        }
    ],
    "minimum-stability": "stable"
}
